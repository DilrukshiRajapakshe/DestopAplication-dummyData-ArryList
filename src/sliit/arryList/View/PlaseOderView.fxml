<AnchorPane fx:id="placeOrderPage" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sliit.arryList.countroller.PlaceOrderController">
   <children>
      <TableView fx:id="plaseOrderTable" layoutX="12.0" layoutY="118.0" onMouseClicked="#ViewCustomerPlaseOrderDetails" prefHeight="268.0" prefWidth="577.0">
        <columns>
          <TableColumn prefWidth="160.0" text="Place order ID" />
          <TableColumn prefWidth="187.0" text="Places Order Date" />
            <TableColumn prefWidth="228.0" text="Customer ID" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="24.0" prefHeight="27.0" prefWidth="200.0" text="Places order details" textFill="#330afc">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="searchText" layoutX="14.0" layoutY="58.0" onKeyReleased="#SearchOrderDetails" prefHeight="35.0" prefWidth="577.0" promptText="Search order details" />
      <ImageView layoutX="562.0" layoutY="64.0">
         <image>
            <Image url="@Image/Search_24px.png" />
         </image>
      </ImageView>
      <Button layoutX="463.0" layoutY="25.0" mnemonicParsing="false" onAction="#goToBack" prefHeight="25.0" prefWidth="126.0" style="-fx-background-color: #40FF00;" text="Back" />
   </children>
